 M("酒馆出征提示信息输出框").value = "冒险者添加成功";
 }
 else {
 for (var i=1; i<酒馆组件列表.length; i++) { 
 if (i < 酒馆[suit_index][hero_index].length) 酒馆[suit_index][hero_index][i] = M(酒馆组件列表[i]).value;
 else 酒馆[suit_index][hero_index].push(M(酒馆组件列表[i]).value);
 }
 Write("酒馆存储", 酒馆); 酒馆 = Read("酒馆存储", 酒馆); RevealTexts();
