 function 首字母大写(str){
 var index = 汉字.indexOf(str[0]);
 if (index === -1) return "";
 return 拼音[index];
 }
 function 大写一致(str1, str2){
 return (M(str1).value == "" || M(str1).value == 首字母大写(str2));
 }
