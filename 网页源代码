 M("冒险者选择下拉框").value = M("冒险者名称输入框").value; M("冒险者名称输入框").value = ""; 
 M("酒馆出征提示信息输出框").value = "冒险者名称修改成功，忽略其他修改";
 }
 else if (hero_index == 0) {
 if (M("冒险者名称输入框").value == "") { M("冒险者出征提示信息输出框").value = "冒险者名称不能为空"; return; }
 for (var i=0;i<酒馆[suit_index].length;i++) if (M("冒险者名称输入框").value == 酒馆[suit_index][i][0]){ 
 M("酒馆出征提示信息输出框").value = "冒险者名称不可以重复"; return; }
