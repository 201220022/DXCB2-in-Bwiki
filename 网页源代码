 $("#对战设置按钮").click(function(){ $("#对战设置容器").slideToggle("fast"); });

 function RevealChoices() {
 配装选择列表 = []; for (var i=0;i<酒馆.length;i++) 配装选择列表.push(酒馆[i][0][0]);
 冒险者选择列表 = []; for (var i=1;i<酒馆[M("配装选择下拉框").selectedIndex].length;i++) 冒险者选择列表.push(酒馆[M("配装选择下拉框").selectedIndex][i][0]);
 SetChoices(M("配装选择下拉框"), 配装选择列表);
 SetChoices(M("冒险者选择下拉框"), ["新增一个冒险者"].concat(冒险者选择列表));
 SetChoices(M("武器首字母下拉框"), 首字母列表);
 SetChoices(M("防具首字母下拉框"), 首字母列表); 
 SetChoices(M("饰品1首字母下拉框"), 首字母列表); 
 SetChoices(M("饰品2首字母下拉框"), 首字母列表);
 SetChoices(M("阶位下拉框"), ["日阶"]);
 SetChoicesTwo(M("分支下拉框"), M("职业下拉框"), 职业选项列表);
 SetChoicesTwo(M("环类型下拉框"), M("环特效下拉框"), 域之环选项列表);
 SetChoicesTwo(M("魔石类型下拉框"), M("魔石数值下拉框"), 魔石选项列表);
 var 武器列表 = [""], 防具列表 = [""], 饰品1列表 = [""], 饰品2列表 = [""];
 for (var i=1;i<装备列表.length;i++){ if (装备列表[i][2] == "防具" && 大写一致("防具首字母下拉框", 装备列表[i][0])) 防具列表.push(装备列表[i][0]); } 
 for (var i=1;i<装备列表.length;i++){ if (装备列表[i][2] == "饰品" && 大写一致("饰品1首字母下拉框", 装备列表[i][0])) 饰品1列表.push(装备列表[i][0]); } 
 for (var i=1;i<装备列表.length;i++){ if (装备列表[i][2] == "饰品" && 大写一致("饰品2首字母下拉框", 装备列表[i][0])) 饰品2列表.push(装备列表[i][0]); } 
 var s = "武器首字母下拉框";
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="战士"&&装备列表[i][2]=="剑"&&大写一致(s, 装备列表[i][0]))武器列表.push(装备列表[i][0]);} 
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="法师"&&装备列表[i][2]=="杖"&&大写一致(s, 装备列表[i][0]))武器列表.push(装备列表[i][0]);} 
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="圣职"&&装备列表[i][2]=="锤"&&大写一致(s, 装备列表[i][0]))武器列表.push(装备列表[i][0]);} 
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="野蛮人"&&装备列表[i][2]=="斧"&&大写一致(s, 装备列表[i][0]))武器列表.push(装备列表[i][0]);} 
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="火枪手"&&装备列表[i][2]=="枪"&&大写一致(s, 装备列表[i][0]))武器列表.push(装备列表[i][0]);} 
 for (var i=1;i<装备列表.length;i++){if(M("分支下拉框").value=="潜行者"&&装备列表[i][2]=="匕首"&&大写一致(s, 装备列表[i][0])) 武器列表.push(装备列表[i][0]);} 
 SetChoices(M("武器下拉框"), 武器列表); 
 SetChoices(M("防具下拉框"), 防具列表); 
 SetChoices(M("饰品1下拉框"), 饰品1列表); 
 SetChoices(M("饰品2下拉框"), 饰品2列表);
 SetChoices(M("站位1下拉框"), [""].concat(冒险者选择列表));
 SetChoices(M("站位2下拉框"), [""].concat(冒险者选择列表));
 SetChoices(M("站位3下拉框"), [""].concat(冒险者选择列表));
 SetChoices(M("站位4下拉框"), [""].concat(冒险者选择列表));
 };
 function RevealSkillChoices(){
 var 技能槽 = ["自奶", "嘲讽", "反击", "治疗", "群攻", "单攻", "单攻", "单攻"]; 
 var settings = [-1, -1, -1, -1, -1, -1, -1, -1];
 var 职业 = M("职业下拉框").value; 
 for (var i=0; i<职业列表.length; i++){ if (职业列表[i][2] == 职业 && 职业列表[i][2] != ""){ 
 var arrays = [];
 if (职业.startsWith("牧星者")) arrays = [职业列表[i]]; 
 else arrays = [职业列表[i], 职业列表[i+1], 职业列表[i+2]]; 
 for (var k=0; k<arrays.length; k++) {
 for (var j=0;j<技能槽.length;j++) { 
 if (((arrays[k][4] == 技能槽[j]) || (j == 0 && arrays[k][4] == "治疗")) && settings[j] == -1) {
 settings[j] = 0; 
 if (j == 0) { M("技能设置名称"+j.toString()).textContent = "治疗自己"; }
 else if (j == 1) { M("技能设置名称"+j.toString()).textContent = "嘲讽"; }
 else if (j == 2) { M("技能设置名称"+j.toString()).textContent = "反击"; }
 else if (j == 3) { M("技能设置名称"+j.toString()).textContent = "治疗友方"; }
 else { M("技能设置名称"+j.toString()).textContent = arrays[k][3]; }
 M("技能设置描述"+j.toString()).textContent = 技能设置描述(技能槽[j], M("技能设置滑块"+j.toString()).value);
 if (j >= 5) { break; }
 }
 }
 }
 }}
 for (var i=0;i<技能槽.length; i++) { 
 if (settings[i]==-1) { M("技能设置容器"+i.toString()).style.display = "none"; }
 else { M("技能设置容器"+i.toString()).style.display = "flex"; }
 }
 M("技能设置描述8").textContent = 优先目标列表[M("技能设置滑块8").value]
 M("技能设置描述9").textContent = 技能循环列表[M("技能设置滑块9").value];
 }
 function RevealTexts(){
 M("酒馆出征提示信息输出框").value = "";
 var suit_index = M("配装选择下拉框").selectedIndex;
 var hero_index = M("冒险者选择下拉框").selectedIndex;
 RevealChoices();
 if (hero_index == 0) { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆组件默认数值列表[i]; } }
 else { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆[suit_index][hero_index][i]; } }
 RevealChoices();
 if (hero_index == 0) { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆组件默认数值列表[i]; } }
 else { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆[suit_index][hero_index][i]; } }
 RevealChoices();
 RevealSkillChoices()
 if (hero_index == 0) { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆组件默认数值列表[i]; } }
 else { for (var i=1; i<酒馆组件列表.length; i++) { M(酒馆组件列表[i]).value = 酒馆[suit_index][hero_index][i]; } }
 }
 
 
 $("#战斗模拟按钮").click(function(){ $("#战斗模拟容器").slideToggle("fast"); });
 $("#开始战斗按钮").click(function(){ 
 Write("配装选择", M("配装选择下拉框").value); 
 Write("木桩生命", M("木桩生命输入框").value); 
 Write("木桩速度", M("木桩速度输入框").value); 
 Write("站位1", M("站位1下拉框").value); 
 Write("站位2", M("站位2下拉框").value); 
 Write("站位3", M("站位3下拉框").value); 
 Write("站位4", M("站位4下拉框").value); 
 生物列表 = [[],[]];
 启动时间轴 = true; tickPrinted = true; M("时间轴输出框").value = "";
 let suit_index = M("配装选择下拉框").selectedIndex;
 let hero_index = [M("站位1下拉框").selectedIndex,M("站位2下拉框").selectedIndex,M("站位3下拉框").selectedIndex,M("站位4下拉框").selectedIndex];
 if (hero_index[0] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[0]]);
 if (hero_index[1] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[1]]);
 if (hero_index[2] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[2]]);
 if (hero_index[3] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[3]]);
 new 生物类("战力测试装置", 1, M("木桩生命输入框").value*1, M("木桩速度输入框").value*1);
 console.log(生物列表);
 for (let i=0;i<生物列表[0].length;i++) { 生物列表[0][i].我方目标 = 生物列表[0][0]; 生物列表[0][i].敌方目标 = 生物列表[1][0]; }
 for (let i=0;i<生物列表[1].length;i++) { 生物列表[1][i].我方目标 = 生物列表[1][0]; 生物列表[1][i].敌方目标 = 生物列表[0][0]; }
 for (let i=0;i<生物列表[0].length;i++) { 生物列表[0][i].准备("战斗开始"); }
 for (let i=0;i<生物列表[1].length;i++) { 生物列表[1][i].准备("战斗开始"); }
 for (tick=1; tick<=2000; tick++) {
 tickPrinted = false;
 for (let i=0;i<生物列表[1].length;i++)if (!End() && 生物列表[1][i].存活==1) 生物列表[1][i].状态时钟();
 for (let i=0;i<生物列表[0].length;i++)if (!End() && 生物列表[0][i].存活==1) 生物列表[0][i].状态时钟();
 for (let i=0;i<生物列表[1].length;i++)if (!End() && 生物列表[1][i].存活==1) 生物列表[1][i].技能时钟();
 for (let i=0;i<生物列表[0].length;i++)if (!End() && 生物列表[0][i].存活==1) 生物列表[0][i].技能时钟();
 if (End()) break;
 }
 启动时间轴 = false;
 
 战力测试([0], "站位1期望输出框", "站位1上限输出框");
 战力测试([1], "站位2期望输出框", "站位2上限输出框");
 战力测试([2], "站位3期望输出框", "站位3上限输出框");
 战力测试([3], "站位4期望输出框", "站位4上限输出框");
 战力测试([0, 1, 2, 3], "全队期望输出框", "全队上限输出框", "胜率输出框");
 })
 
 function 战力测试(inlist, out1, out2, out3) {
 let 输出列表 = []; let 总胜场 = 0;
 for (let round = 0; round<100; round++){
 生物列表 = [[],[]];
 let suit_index = M("配装选择下拉框").selectedIndex;
 let hero_index = [M("站位1下拉框").selectedIndex,M("站位2下拉框").selectedIndex,M("站位3下拉框").selectedIndex,M("站位4下拉框").selectedIndex];
 if (inlist.includes(0) && hero_index[0] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[0]]);
 if (inlist.includes(1) && hero_index[1] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[1]]);
 if (inlist.includes(2) && hero_index[2] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[2]]);
 if (inlist.includes(3) && hero_index[3] != 0) new 生物类("冒险者", 0, 酒馆[suit_index][hero_index[3]]);
 new 生物类("战力测试装置", 1, M("木桩生命输入框").value*1, M("木桩速度输入框").value*1);
 for (let i=0;i<生物列表[0].length;i++) { 生物列表[0][i].我方目标 = 生物列表[0][0]; 生物列表[0][i].敌方目标 = 生物列表[1][0]; }
 for (let i=0;i<生物列表[1].length;i++) { 生物列表[1][i].我方目标 = 生物列表[1][0]; 生物列表[1][i].敌方目标 = 生物列表[0][0]; }
 for (let i=0;i<生物列表[0].length;i++) { 生物列表[0][i].准备("战斗开始"); }
 for (let i=0;i<生物列表[1].length;i++) { 生物列表[1][i].准备("战斗开始"); }
 for (tick=1; tick<=2000; tick++) {
 for (let i=0;i<生物列表[1].length;i++)if (!End() && 生物列表[1][i].存活==1) 生物列表[1][i].状态时钟();
 for (let i=0;i<生物列表[0].length;i++)if (!End() && 生物列表[0][i].存活==1) 生物列表[0][i].状态时钟();
 for (let i=0;i<生物列表[1].length;i++)if (!End() && 生物列表[1][i].存活==1) 生物列表[1][i].技能时钟();
 for (let i=0;i<生物列表[0].length;i++)if (!End() && 生物列表[0][i].存活==1) 生物列表[0][i].技能时钟();
 if (End()) break;
 }
 输出列表.push(生物列表[1][0].属性("体质")*10 - 生物列表[1][0].当前生命);
 总胜场 += (生物列表[1][0].当前生命 == 0);
 }
 输出列表.sort((a, b) => b - a);
 M(out1).value = Int((输出列表[49] + 输出列表[50])/2);
 M(out2).value = Int(输出列表[9]);
 if (out3!=undefined) M(out3).value = (总胜场).toString() + "%";
 }
 </script>
</p>
