 M("冒险者出征提示信息输出框").value = "不可以含有英文标点"; return; }
 var info = []; for (var i=0; i<酒馆组件列表.length; i++) { info.push(M(酒馆组件列表[i]).value); }
 酒馆[suit_index].push(info);
 Write("酒馆存储", 酒馆); 酒馆 = Read("酒馆存储", 酒馆); RevealTexts(); 
