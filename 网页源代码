 $("input").bind("input propertychange", function () { 
 $("#"+this.id).on("focus",function() { if(Mtype(this.id)=="百分框" && M(this.id).value.endsWith("%")) { M(this.id).value = M(this.id).value.slice(0, -1); } });
 $("#"+this.id).on("blur", function() { 
 if(Mtype(this.id)=="百分框" && M(this.id).value !== "" && !M(this.id).value.endsWith("%")) { M(this.id).value = M(this.id).value + "%"; }
 if (M("环类型下拉框").value=="地") { M("环数值百分框").value = M("环数值百分框").value.replace(/%/, ""); } 
 });
 Inputing(this.id); }
 )
