 $("#酒馆出征按钮").click(function(){ $("#酒馆出征容器").slideToggle("fast"); });
 $("#酒馆出征保存按钮").click(function(){
 var suit_index = M("配装选择下拉框").selectedIndex;
 var hero_index = M("冒险者选择下拉框").selectedIndex;
 if (M("配装名称输入框").value != "" && M("配装名称输入框").value != 酒馆[suit_index][0][0]) {
 for (var i=0;i<20;i++) if (M("配装名称输入框").value == 酒馆[i][0][0]){ M("酒馆出征提示信息输出框").value = "配装名称不可以重复"; return; }
 if (M("配装名称输入框").value.includes(",") || M("配装名称输入框").value.includes("\"")){ M("酒馆出征提示信息输出框").value = "不可以含有英文标点"; return; }
 酒馆[suit_index][0][0] = M("配装名称输入框").value; 
